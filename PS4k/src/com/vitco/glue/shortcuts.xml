<?xml version="1.0" encoding="UTF-8"?>
<head xmlns="http://www.unfine.de/schema/shortcuts">
    <global>
        <!--History-->
        <shortcut ctrl="yes" alt="no" shift="no" key="Z" action="global_action_undo" caption="global_undo_caption" />
        <shortcut ctrl="yes" alt="no" shift="yes" key="Z" action="global_action_redo" caption="global_redo_caption" />
        <!--Windows-->
        <shortcut ctrl="yes" alt="no" shift="no" key="F2" action="toggle_animation_mode" caption="toggle_animation_mode" />
        <shortcut ctrl="yes" alt="no" shift="no" key="1" action="console_state-action_show" caption="console_caption" />
        <shortcut ctrl="yes" alt="no" shift="no" key="2" action="shortcut-mg_state-action_show" caption="shortcutManager_caption" />
        <shortcut ctrl="yes" alt="no" shift="no" key="3" action="side-view_state-action_show1" caption="xyView_caption" />
        <shortcut ctrl="yes" alt="no" shift="no" key="4" action="side-view_state-action_show2" caption="xzView_caption" />
        <shortcut ctrl="yes" alt="no" shift="no" key="5" action="side-view_state-action_show3" caption="yzView_caption" />
        <shortcut ctrl="yes" alt="no" shift="no" key="6" action="snapshot_state-action_show" caption="snapshot_caption" />
        <shortcut ctrl="yes" alt="no" shift="no" key="7" action="personal_state-action_show" caption="personal_caption" />
        <shortcut ctrl="yes" alt="no" shift="no" key="8" action="layer_state-action_show" caption="layer_caption" />
        <shortcut ctrl="yes" alt="no" shift="no" key="9" action="colorpicker_state-action_show" caption="colorpicker_caption" />
        <shortcut ctrl="yes" alt="no" shift="no" key="0" action="mainview_state-action_show" caption="mainView_caption" />
        <shortcut ctrl="yes" alt="no" shift="no" key="-" action="colorPalette_state-action_show" caption="colorPalette_caption" />
        <!--File-->
        <shortcut ctrl="yes" alt="no" shift="no" key="N" action="new_file_action" caption="new_file_caption" />
        <shortcut ctrl="yes" alt="no" shift="no" key="O" action="load_file_action" caption="load_file_caption" />
        <shortcut ctrl="yes" alt="no" shift="no" key="I" action="import_file_action" caption="import_file_caption" />
        <shortcut ctrl="yes" alt="no" shift="yes" key="E" action="export_file_action" caption="export_file_caption" />
        <shortcut ctrl="yes" alt="no" shift="no" key="S" action="quick_save_file_action" caption="quick_save_file_caption" />
        <shortcut ctrl="yes" alt="no" shift="yes" key="S" action="save_file_action" caption="save_file_caption" />
        <!--Tools-->
        <shortcut ctrl="no" alt="no" shift="no" key="1" action="voxel_mode_select_type_view" caption="voxel_mode_select_type_view_caption" />
        <shortcut ctrl="no" alt="no" shift="no" key="2" action="voxel_mode_select_type_draw" caption="voxel_mode_select_type_draw_caption" />
        <shortcut ctrl="no" alt="no" shift="no" key="3" action="voxel_mode_select_type_erase" caption="voxel_mode_select_type_erase_caption" />
        <shortcut ctrl="no" alt="no" shift="no" key="4" action="voxel_mode_select_type_picker" caption="voxel_mode_select_type_picker_caption" />
        <shortcut ctrl="no" alt="no" shift="no" key="5" action="voxel_mode_select_type_floodfill_tool" caption="voxel_mode_select_type_floodfill_tool_caption" />
        <shortcut ctrl="no" alt="no" shift="no" key="6" action="voxel_mode_select_type_color_changer" caption="voxel_mode_select_type_color_changer_caption" />
        <shortcut ctrl="no" alt="no" shift="no" key="7" action="voxel_mode_select_type_select_tool" caption="voxel_mode_select_type_select_tool_caption" />
        <shortcut action="toggle_voxel_snap" caption="toggle_voxel_snap_caption" />
        <!--Selection Interaction-->
        <shortcut ctrl="yes" alt="no" shift="no" key="X" action="selection_tool_cut" caption="selection_tool_cut_caption" />
        <shortcut ctrl="yes" alt="no" shift="no" key="C" action="selection_tool_copy" caption="selection_tool_copy_caption" />
        <shortcut ctrl="yes" alt="no" shift="no" key="V" action="selection_tool_paste" caption="selection_tool_paste_caption" />

        <shortcut ctrl="no" alt="no" shift="no" key="ESC" action="selection_tool_deselect" caption="selection_tool_deselect_caption" />
        <shortcut ctrl="no" alt="no" shift="no" key="DEL" action="selection_tool_delete" caption="selection_tool_delete_caption" />

        <shortcut ctrl="yes" alt="no" shift="no" key="A" action="selection_tool_select_all" caption="selection_tool_select_all_caption" />
        <shortcut ctrl="yes" alt="no" shift="yes" key="W" action="selection_tool_expand_selection" caption="selection_tool_expand_selection_caption" />
        <shortcut ctrl="yes" alt="no" shift="yes" key="N" action="selection_tool_as_new_layer" caption="selection_tool_as_new_layer_caption" />
        <shortcut ctrl="yes" alt="no" shift="yes" key="R" action="selection_tool_recolor" caption="selection_tool_recolor_caption" />
        <shortcut ctrl="yes" alt="no" shift="yes" key="T" action="selection_tool_retexture" caption="selection_tool_retexture_caption" />

        <shortcut ctrl="no" alt="no" shift="no" key="ENTER" action="selection_tool_finalize_shifting" caption="selection_tool_finalize_shifting_caption" />
        <shortcut ctrl="yes" alt="no" shift="no" key="ENTER" action="selection_tool_finalize_shifting_as_copy" caption="selection_tool_finalize_shifting_as_copy_caption" />

        <!--Color Palette-->
        <shortcut ctrl="no" alt="no" shift="no" key="A" action="color-palette_color_left" caption="color-palette_color_left_caption" />
        <shortcut ctrl="no" alt="no" shift="no" key="D" action="color-palette_color_right" caption="color-palette_color_right_caption" />
        <shortcut ctrl="no" alt="no" shift="no" key="W" action="color-palette_color_up" caption="color-palette_color_up_caption" />
        <shortcut ctrl="no" alt="no" shift="no" key="S" action="color-palette_color_down" caption="color-palette_color_down_caption" />

        <!--Custom-->
        <shortcut action="swap_mainView_with_xyView" caption="swap_mainView_with_xyView_caption" />
        <shortcut action="swap_mainView_with_xzView" caption="swap_mainView_with_xzView_caption" />
        <shortcut action="swap_mainView_with_yzView" caption="swap_mainView_with_yzView_caption" />
        <shortcut ctrl="no" alt="no" shift="no" key="P" action="pick_color_under_mouse_as_current_color" caption="pick_color_under_mouse_as_current_color_caption" />

        <!--Move Selection-->
        <shortcut ctrl="no" alt="no" shift="no" key="UP" action="shift_selected_voxels_up" caption="shift_selected_voxels_up_caption" />
        <shortcut ctrl="no" alt="no" shift="no" key="DOWN" action="shift_selected_voxels_down" caption="shift_selected_voxels_down_caption" />
        <shortcut ctrl="no" alt="no" shift="no" key="LEFT" action="shift_selected_voxels_left" caption="shift_selected_voxels_left_caption" />
        <shortcut ctrl="no" alt="no" shift="no" key="RIGHT" action="shift_selected_voxels_right" caption="shift_selected_voxels_right_caption" />
        <shortcut ctrl="no" alt="no" shift="no" key="PAGE_DOWN" action="shift_selected_voxels_in" caption="shift_selected_voxels_in_caption" />
        <shortcut ctrl="no" alt="no" shift="no" key="PAGE_UP" action="shift_selected_voxels_out" caption="shift_selected_voxels_out_caption" />

    </global>

    <frame name="mainView">
        <shortcut ctrl="yes" alt="no" shift="no" key="SPACE" action="reset_main_view_camera" caption="reset_camera_main-view_caption" />
        <shortcut ctrl="yes" alt="no" shift="yes" key="I" action="mainview_zoom_in" caption="main-view_zoom_in_caption" />
        <shortcut ctrl="yes" alt="no" shift="yes" key="O" action="mainview_zoom_out" caption="main-view_zoom_out_caption" />
    </frame>

    <frame name="xyView">
        <shortcut action="sideview_set_plane_to_zero1" caption="sideview_set_plane_to_zero_caption" />
        <shortcut ctrl="yes" key="UP" action="sideview_move_plane_in1" caption="sideview_move_plane_in_caption" />
        <shortcut ctrl="yes" key="DOWN" action="sideview_move_plane_out1" caption="sideview_move_plane_out_caption" />
        <shortcut ctrl="yes" alt="no" shift="no" key="SPACE" action="sideview_reset_view1" caption="sideview_reset_view_tooltip" />
        <shortcut ctrl="yes" alt="no" shift="yes" key="I" action="sideview_zoom_in_tb1" caption="sideview_zoom_out" />
        <shortcut ctrl="yes" alt="no" shift="yes" key="O" action="sideview_zoom_out_tb1" caption="sideview_zoom_in" />
    </frame>

    <frame name="xzView">
        <shortcut action="sideview_set_plane_to_zero2" caption="sideview_set_plane_to_zero_caption" />
        <shortcut ctrl="yes" key="UP" action="sideview_move_plane_in2" caption="sideview_move_plane_in_caption" />
        <shortcut ctrl="yes" key="DOWN" action="sideview_move_plane_out2" caption="sideview_move_plane_out_caption" />
        <shortcut ctrl="yes" alt="no" shift="no" key="SPACE" action="sideview_reset_view2" caption="sideview_reset_view_tooltip" />
        <shortcut ctrl="yes" alt="no" shift="yes" key="I" action="sideview_zoom_in_tb2" caption="sideview_zoom_out" />
        <shortcut ctrl="yes" alt="no" shift="yes" key="O" action="sideview_zoom_out_tb2" caption="sideview_zoom_in" />
    </frame>

    <frame name="yzView">
        <shortcut action="sideview_set_plane_to_zero3" caption="sideview_set_plane_to_zero_caption" />
        <shortcut ctrl="yes" key="UP" action="sideview_move_plane_in3" caption="sideview_move_plane_in_caption" />
        <shortcut ctrl="yes" key="DOWN" action="sideview_move_plane_out3" caption="sideview_move_plane_out_caption" />
        <shortcut ctrl="yes" alt="no" shift="no" key="SPACE" action="sideview_reset_view3" caption="sideview_reset_view_tooltip" />
        <shortcut ctrl="yes" alt="no" shift="yes" key="I" action="sideview_zoom_in_tb3" caption="sideview_zoom_out" />
        <shortcut ctrl="yes" alt="no" shift="yes" key="O" action="sideview_zoom_out_tb3" caption="sideview_zoom_in" />
    </frame>

    <frame name="console">
        <shortcut ctrl="no" alt="yes" shift="yes" key="A" action="console_toggle_auto_show" caption="console_auto-show_caption" />
        <shortcut ctrl="no" alt="yes" shift="yes" key="S" action="console_toggle_auto_scroll" caption="console_auto-scroll_caption" />
        <shortcut ctrl="no" alt="yes" shift="yes" key="C" action="console_action_clear" caption="console_clear_caption" />
    </frame>

    <frame name="snapshot">
    </frame>

    <frame name="shortcutManager">
    </frame>

    <frame name="personal">
    </frame>

    <frame name="layer">
        <shortcut ctrl="yes" alt="no" shift="no" key="DEL" action="layer-frame_remove-layer" caption="layer-frame_remove-layer_caption" />
        <shortcut ctrl="yes" alt="yes" shift="no" key="N" action="layer-frame_add-layer" caption="layer-frame_add-layer_caption" />
        <shortcut ctrl="yes" alt="yes" shift="no" key="U" action="layer-frame_move-layer-up" caption="layer-frame_move-layer-up_caption" />
        <shortcut ctrl="yes" alt="yes" shift="no" key="D" action="layer-frame_move-layer-down" caption="layer-frame_move-layer-down_caption" />
    </frame>

    <frame name="colorpicker">
    </frame>

    <frame name="colorslider">
    </frame>

    <frame name="textureManager">
        <shortcut ctrl="yes" alt="yes" shift="no" key="A" action="texturemg_action_add" caption="texturemg_add_caption" />
        <shortcut ctrl="yes" alt="yes" shift="no" key="D" action="texturemg_action_remove" caption="texturemg_remove_caption" />
        <shortcut ctrl="yes" alt="yes" shift="no" key="R" action="texturemg_action_replace" caption="texturemg_replace_caption" />
        <shortcut ctrl="yes" alt="yes" shift="no" key="E" action="texturemg_action_export" caption="texturemg_export_caption" />
    </frame>

    <frame name="hexPicker">
    </frame>

    <frame name="colorPalette">
    </frame>

    <frame name="colorAdjuster">
    </frame>
</head>